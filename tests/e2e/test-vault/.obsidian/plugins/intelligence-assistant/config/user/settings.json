  "mcp": {
    "servers": [
      {
        "name": "Demo Auto MCP",
        "command": "node demo-mcp.js",
        "args": [
          "--port",
          "8080"
        ],
        "env": {
          "DEMO": "true"
        },
        "enabled": true,
        "connectionMode": "auto",
        "cachedTools": [
          {
            "name": "demo-tool",
            "description": "Seeded demo tool",
            "args": []
